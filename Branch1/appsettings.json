{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "SeqServerUrl": "http://localhost:5341",
    "LogPath": "C:\\LogFiles\\Identity\\",
    "LogstashgUrl": null,
    "ElasticConfiguration": "http://elasticsearch:9200",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Error"
      }
    }
  },
  "AllowedHosts": "*"
}
